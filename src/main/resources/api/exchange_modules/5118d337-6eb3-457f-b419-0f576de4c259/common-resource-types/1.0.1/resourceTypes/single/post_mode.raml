#%RAML 1.0 ResourceType

  description: <<postSummary>>
  post:      
    description: <<postDescription>>
    body:
      type: <<postRecordType>>
      example: <<postRecordExample>>      
    is: 
      - jsonResponse: {statusCode: 201, responseDescription: "This status code will be returned when the request has succeeded and a new resource has been created as a result", recordType: <<postResponseType>>, recordExample: <<postResponseExample>>}
      - postErrorResponse
